<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="NUT_DBUS_URL.DeviceManager">
    <signal name="deviceAdded">
      <arg name="objectpath" type="o" direction="out"/>
    </signal>
    <signal name="deviceRemoved">
      <arg name="objectpath" type="o" direction="out"/>
    </signal>
    <method name="getDeviceList">
      <arg type="ao" direction="out"/>
    </method>
  </interface>
  <interface name="NUT_DBUS_URL.Device">
    <signal name="environmentChangedActive">
      <arg name="previous" type="s" direction="out"/>
    </signal>
    <signal name="environmentsUpdated">
      <arg name="device" type="o" direction="out"/>
    </signal>
    <signal name="stateChanged">
    </signal>
    <method name="getEnvironments">
      <arg type="ao" direction="out"/>
    </method>
    <method name="enable">
      <arg type="b" direction="out"/>
    </method>
    <method name="disable">
      <arg type="b" direction="out"/>
    </method>
  </interface>
  <interface name="NUT_DBUS_URL.Environment">
    <signal name="interfacesUpdated">
    </signal>
    <method name="getInterfaces">
      <arg type="ao" direction="out"/>
    </method>
  </interface>
  <interface name="NUT_DBUS_URL.Interface">
    <signal name="activeStateChanged">
    </signal>
    <method name="setNetmask">
      <arg name="Netmask" type="u" direction="in"/>
    </method>
    <method name="setGateway">
      <arg name="Gateway" type="u" direction="in"/>
    </method>
    <method name="activate">
    </method>
    <method name="deactivate">
    </method>
  </interface>
</node>
